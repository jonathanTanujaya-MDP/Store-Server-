{
  "version": 2,
  "name": "manajemen-stock-backend",
  "builds": [
    {
      "src": "src/app-simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app-simple.js": {
      "maxDuration": 10
    }
  }
}
